#include <stdio.h>


### if a number if power of 2 only one bit will be set in that number 
int ispowerof2(int num){
  if (num & (num -1)) == 0{
  return 1}
return 0;
}

int main(){
num = 10;
bool var;
var =ispowerof2(num);
if (var == 1){
printf("Number is power of 2");
}
else if (var == 0){
printf("Number is not power of 2");
}

return 0;
}
